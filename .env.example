# Vista previa sin auth (para demo / entrega visual al cliente)
# true = app visible sin configurar Clerk; false = requiere Clerk
VITE_DISABLE_CLERK=true

# Clerk (obtener en https://dashboard.clerk.com) — solo si VITE_DISABLE_CLERK no es true
VITE_CLERK_PUBLISHABLE_KEY=pk_test_xxxxx

# API Backend (Python) - URL base cuando esté desplegado
VITE_API_URL=http://localhost:8000

# Opcionales
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
